{
  "version": "1.1",
  "name": "role-assignment-handler",
  "func_name": "RoleAssignmentHandler::RoleAssignmentHandler.Function::FunctionHandler",
  "resource_type": "lambda",
  "iam_role_name": "role-assignment-handler-role",
  "runtime": "dotnet8",
  "memory": 128,
  "timeout": 100,
  "lambda_path": "lambdas\\role-assignment-handler",
  "dependencies": [
    {
      "resource_name": "waiters-list",
      "resource_type": "dynamodb_table"
    }
  ],
  "event_sources": [
    {
      "resource_type": "cognito_idp",
      "trigger": "PostConfirmation",
      "pool_name": "cognito-user-pool"
    }
  ],
  "env_variables": {
    "WAITERS_TABLE_NAME": "waiters-list",
    "WAITER_ROLE_NAME": "Waiter",
    "CUSTOMER_ROLE_NAME": "Customer"
  },
  "publish_version": true,
  "alias": "${lambdas_alias_name}",
  "url_config": {},
  "ephemeral_storage": 512,
  "tags": {}
}